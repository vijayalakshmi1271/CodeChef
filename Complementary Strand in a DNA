# cook your dish here
t = int(input())

while t > 0:
    n = int(input())
    s = input()
    ans = ""
    for i in s:
        if i == "A":
            ans += "T"
        if i == "T":
            ans += "A"
        if i == "C":
            ans += "G"
        if i == "G":
            ans += "C"
    print(ans)
    t -= 1
